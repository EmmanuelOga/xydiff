<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xyd="http://www.nflc.org/schema/xydiff/" attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
